    "main": "webpack.config.js",
    "scripts": {
        "app": "npm run rmrf && webpack -p",
        "prod": "webpack -p",
        "dev": "npm run rmrf && webpack -p -w",
        "page": "cd modules && browser-sync start --server --files './*.html'",
        "index": "browser-sync start --server --files './index.html'",
        "dev & prod": "??? webpack --config webpack.prod.js ???",
        "hot_modules_loader": "??? webpack hot HMR ???",
        "rmrf": "rimraf build/js",
        "linux_rmrf": "rm -rf build/js",
        "windows_clear": "rimraf build/js",
        "xapp": "npm run build && cd build && browser-sync start --server --files 'index.html'",
        "open": "start  http://localhost:7777/posts/1",
        "bat": "start start-browser.bat",
        "starts": "start http://localhost:7777/datas/ && start http://localhost:7777/Info/",
        "test": "cd modules && browser-sync start --server --files './*.*'",
        "infos": "echo JSON-server ready running...\n",
        "jsondb": "json-server --watch db.json --port 7777",
        "db": "npm run infos && npm run open && npm run jsondb"
    },
    "bin": {
        "f9-fv": "f9-fv",
        "bin": "./bin.js"
    },
